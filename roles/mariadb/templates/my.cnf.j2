# MariaDB Configuration
[client]
default-character-set = utf8

[mysql]
default-character-set = utf8

[mysqld]
character-set-server  = utf8
collation-server      = utf8_general_ci
character_set_server  = utf8
collation_server      = utf8_general_ci
max_allowed_packet    = 128M
max_connections       = 500
skip-log-bin
